            <Button mnemonicParsing="false" onMouseClicked="#play" prefHeight="35.0" prefWidth="150.0" text="Start" textAlignment="CENTER" />
         </children>
         <padding>
            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
         </padding>
      </HBox>
   </children>
</VBox>
