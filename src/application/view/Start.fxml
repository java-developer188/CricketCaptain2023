<VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.StartController">
   <children>
      <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="2.0" VBox.vgrow="ALWAYS">
         <children>
            <ImageView fitHeight="600.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true" HBox.hgrow="ALWAYS">
               <effect>
                  <Glow />
               </effect>
               <image>
                  <Image url="@../../cc23.jpeg" />
               </image>
               <HBox.margin>
                  <Insets bottom="100.0" left="100.0" right="100.0" top="100.0" />
               </HBox.margin>
            </ImageView>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="40.0" spacing="2.0" VBox.vgrow="ALWAYS">
         <children>
            <TextField fx:id="playerName" maxHeight="-Infinity" maxWidth="1.7976931348623157E308" prefHeight="40.0" promptText="Player Name">
               <font>
